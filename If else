/*Take a character input from the user and
- if a character is between 0 and 9 then it should print 
- if a character is between a and z then it should print 
- if a character is between A and Z then it should print "This is uppercase character."*/

// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main() {
    char c;
    cin>>c;
    if(c>='0' && c<='9'){
        cout<<"This is numeric character."<<endl;
        
    }
    else if(c>='a' && c<='z'){
        cout<<"This is lowercase character."<<endl;
    }
    else{
        cout<<"This is uppercase character."<<endl;
    }

    return 0;
}

/*Take a character input from the user and
- if a character is between 0 and 9 then it should print 
- if a character is between a and z then it should print 
- if a character is between A and Z then it should print "This is uppercase character."*/
